{
    "name": "dogma/dogma",
    "type": "library",
    "description": "Collection of OO libraries sharing the same base and the same ideology - simplicity, self-explaining API, good design. Includes Database, Datasheet, XML/HTML DOM, HTTP, Filesystem and Email tools.",
    "keywords": ["datasheet","database","xml","html","dom","http","filesystem","mail","imap"],
    "homepage": "http://github.com/paranoiq/dogma",
    "license": ["BSD-3"],
    "authors": [
        {
            "name": "Vlasta Neubauer",
            "homepage": "https://twitter.com/paranoiq"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "nette/utils": "~2.4",
        "nette/reflection": "~2.4",
        "tracy/tracy": "~2.4"
    },
    "require-dev": {
        "nette/tester": "~1.7",
        "phing/phing": "~2.16",
        "squizlabs/php_codesniffer": "~2.7",
        "jakub-onderka/php-console-highlighter": "0.3.1",
        "jakub-onderka/php-parallel-lint": "0.8.0",
        "phpstan/phpstan": "0.4.2"
    },
    "autoload": {
        "files": ["Dogma/loader.php"]
    },
    "config": {
        "bin-dir": "bin"
    }
}
