{
    "name": "dogma/dogma",
    "type": "library",
    "description": "The standard library you wish PHP had.",
    "homepage": "http://github.com/paranoiq/dogma",
    "license": ["BSD-3-Clause"],
    "authors": [
        {
            "name": "Vlasta Neubauer",
            "homepage": "http://www.paranoiq.cz"
        }
    ],
    "require": {
        "php-64bit": "^7.2.0",
        "ext-calendar":"*",
        "nette/http": "~2.4|~3.0",
        "nette/utils": "~2.4|~3.0",
        "tracy/tracy": "~2.4"
    },
    "require-dev": {
        "dogma/dogma-dev": "0.1.14"
    },
    "suggest": {
        "ext-dom": "for Dom",
        "ext-iconv": "for Str",
        "ext-intl": "for Str and Language",
        "ext-mbstring": "for Str",
        "symfony/polyfill-intl-icu": "for Str and Language",
        "symfony/polyfill-intl-normalizer": "for Str",
        "symfony/polyfill-mbstring": "for Str"
    },
    "autoload": {
        "classmap": ["src", "tests"]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "alpha"
}
