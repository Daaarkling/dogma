{
    "name": "dogma/dogma",
    "type": "library",
    "description": "Collection of libraries sharing the same base and the same ideology - simplicity, self-explaining API, good design. Includes Database, Data sheet, XML/HTML DOM, HTTP, Filesystem and Email tools.",
    "homepage": "http://github.com/paranoiq/dogma",
    "license": ["BSD-3"],
    "authors": [
        {
            "name": "Vlasta Neubauer",
            "homepage": "http://www.paranoiq.cz"
        }
    ],
    "require": {
        "php": ">=7.1",
        "nette/http": "~2.4",
        "nette/reflection": "~2.4",
        "nette/php-generator": "~2.4",
        "nette/utils": "~2.4",
        "tracy/tracy": "~2.4"
    },
    "require-dev": {
        "consistence/coding-standard": "0.13.0",
        "jakub-onderka/php-console-highlighter": "~0.3",
        "jakub-onderka/php-parallel-lint": "~0.8",
        "nette/tester": "~1.7",
        "phing/phing": "~2.16",
        "phpstan/phpstan": "~0.6",
        "slevomat/coding-standard": "dev-2.0-dev#06c9cbb",
        "squizlabs/php_codesniffer": "~2.7"
    },
    "autoload": {
        "files": ["Dogma/loader.php"]
    },
    "config": {
        "bin-dir": "bin"
    }
}
